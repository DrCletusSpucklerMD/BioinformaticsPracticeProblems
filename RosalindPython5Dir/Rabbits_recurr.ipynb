   "source": [
    "def Rabbits(n, k):\n",
    "    if n <= 2:\n",
    "        total_rabbits = 1\n",
    "        return total_rabbits\n",
    "    else:\n",
    "        total_rabbits = Rabbits(n-1,k) + k*Rabbits(n-2,k)\n",
    "    return total_rabbits"
   ],
